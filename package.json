{
  "name": "ts-express-server",
  "version": "1.0.0",
  "description": "This is a simple express typescript server setup with authentication.",
  "repository": "https://github.com/ZVER3D/ts-express-server",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
